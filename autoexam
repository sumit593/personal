// üîá Console ‡§ï‡•ã ‡§ö‡•Å‡§™ ‡§ï‡§∞ ‡§¶‡•ã
console.log = function () {};
console.error = function () {};

setInterval(() => {
  try {
    const iframe = document.getElementById("stallexam");
    if (!iframe) return;

    const iframeDoc = iframe.contentDocument || iframe.contentWindow.document;
    if (!iframeDoc) return;

    const scripts = iframeDoc.querySelectorAll("script");
    let matchRadioNum = null;

    for (const script of scripts) {
      const code = script.innerHTML;
      const match = code.match(/document\.getElementById\("radio(\d+)"\)\.style\.background\s*=\s*["']#8ac007["']/i);
      if (match) {
        matchRadioNum = match[1];
        break;
      }
    }

    if (!matchRadioNum) return;

    const radioId = "stallradio" + matchRadioNum;
    const radioBtn = iframeDoc.getElementById(radioId);
    if (!radioBtn) return;

    radioBtn.checked = true;

    const confirmBtn = iframeDoc.getElementById("confirmbut");
    if (!confirmBtn) return;

    confirmBtn.disabled = false;
    confirmBtn.removeAttribute("disabled");
    confirmBtn.click();

  } catch (err) {
    // errors ‡§≠‡•Ä ‡§ö‡•Å‡§™ ‡§ï‡§∞‡§æ ‡§¶‡§ø‡§è ‡§ó‡§è ‡§π‡•à‡§Ç
  }
}, 1000); // ‡§π‡§∞ 1 ‡§∏‡•á‡§ï‡§Ç‡§° ‡§Æ‡•á‡§Ç auto-run
